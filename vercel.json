{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",  
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**"]  
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",      
        "dest": "src/index.js", 
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]  
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "src/index.js"  
      }
    ]
  }
  